{{#if user.businessname}}
	<h3>Here is the current Queue: </h3>  
	<table>
	  <tr>
		<th>Position</th>
		<th>Last Name</th>
		<th>First Name</th>
		<th>Email</th>
		<th>Phone Number</th>
		<th>Time</th>
		<th>Transaction Completed</th>
	  </tr>
		{{#each queuedata }}
		<tr>
			<td>{{inc @index}}</td>
			<td>{{lastname}}</td>
			<td>{{firstname}}</td>
			<td>{{email}}</td>
			<td>{{phonenumber}}</td>
			<td>{{date}}</td>
			<td>
				<form action="/merchant/completeTransaction" method="post">
					<input type="hidden" class="form-control" name="businessname" value="{{user.businessname}}">
					<input type="hidden" class="form-control" name="email" value={{user.username}}>
					<input type="hidden" name="id" type="submit" class="form-control" value={{_id}}>
					<button type="submit" class="btn btn-lg btn-info">Complete</button>
				</form>
			</td>
		</tr>
		{{/each}}
	</table>
{{/if}}